<div id="projects-content">
	{{#each projects}}
		<div id="project{{@key}}" class="project">
			<div id="project-content">
				<div id="project-content-text">
					<h1>{{this.title}}</h1>
					<h2>{{this.desc}}</h2>
				</div>
			</div>
			<iframe src="//player.vimeo.com/video/{{this.vimeo_id}}?background=1" width="100%" height="100%" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
		</div>
	{{/each}}
	
</div>
